			<string key="concept:name" value="comment_pull_request" />
			<date key="time:timestamp" value="2025-07-18T09:19:23+00:00" />
			<string key="attributes" value="[{'name': 'comment', 'value': '&gt; add &quot;make a separate PR that updates the news&quot; a to do item for the release checklist\r\n\r\nNot sure what you mean ? it\'s already an item in the checklist, see https://github.com/scikit-learn/scikit-learn/pull/31762#issue-3232800116'}]" />
			<string key="relationships" value="[{'objectId': 'Jérémie du Boisberranger', 'qualifier': 'commented-by'}, {'objectId': '31780', 'qualifier': 'on-pull-request'}]" />
		</event>
		<event>
