			<string key="concept:name" value="comment_pull_request" />
			<date key="time:timestamp" value="2025-07-09T08:24:41+00:00" />
			<string key="attributes" value="[{'name': 'comment', 'value': &quot;Since right now fitting an empty pipeline fails anyway, I'm happy with this PR since it makes the error message much more user friendly and doesn't really change any behavior. &quot;}]" />
			<string key="relationships" value="[{'objectId': 'Adrin Jalali', 'qualifier': 'commented-by'}, {'objectId': '31723', 'qualifier': 'on-pull-request'}]" />
		</event>
		<event>
